<%= simple_form_for(@expense) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :amount %>
    <%= f.association :receipt %>
    <%= f.input :transaction_date %>
    <%= f.association :user %>
    <%= f.association :week %>
    <%= f.input :expense_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
